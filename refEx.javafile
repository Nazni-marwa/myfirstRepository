import java.util.Scanner;

public class refEx {
    static int x= 5;
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println(x);
        foo();
        bar();
    }
    static void foo(){
        System.out.println(x);
        x=10;
    }
    static void bar(){
        System.out.println(x);
    }
}
